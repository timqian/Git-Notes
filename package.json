{
  "name": "react-webpack-gulp-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "gulp dev",
    "dist": "NODE_ENV=production gulp dist",
    "test": "jest"
  },
  "jest": {
    "testDirectoryName": "tests",
    "testFileExtensions": [
      "js",
      "jsx"
    ],
    "scriptPreprocessor": "<rootDir>/tests/preprocessor.js",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "preprocessor.js"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ]
  },
  "devDependencies": {
    "babel-core": "^4.7.3",
    "babel-loader": "^4.1.0",
    "css-loader": "^0.9.1",
    "extract-text-webpack-plugin": "^0.3.8",
    "gulp": "^3.8.11",
    "gulp-util": "^3.0.4",
    "jest-cli": "^0.4.0",
    "react-hot-loader": "^1.1.7",
    "require-dir": "^0.1.0",
    "style-loader": "^0.8.3",
    "webpack": "^1.7.2",
    "webpack-dev-server": "^1.7.0"
  },
  "dependencies": {
    "react": "^0.12.2"
  }
}
